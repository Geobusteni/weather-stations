{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "kst-weather-stations/map",
    "version": "1.0.0",
    "title": "Weather Stations Map",
    "category": "widgets",
    "icon": "location",
    "description": "Display weather stations on a map.",
    "keywords": ["weather", "map", "stations"],
    "supports": {
        "html": false,
        "align": false
    },
    "textdomain": "kst-weather-stations",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css",
    "viewScript": "file:./view.js",
    "attributes": {
        "title": {
            "type": "string",
            "default": ""
        },
        "selectedStations": {
            "type": "array",
            "default": []
        },
        "showAllStations": {
            "type": "boolean",
            "default": false
        }
    }
}
